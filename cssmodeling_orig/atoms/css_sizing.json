{
	"variables":{

		"phi":{
			"base":"phi",
			"scheme":"phi-values",
			"values":[
				"61.803%","_@_phi / 2","100% - _@_phi","( 100% - _@_phi ) / 2",
				"61.803vh","_@_phi-vh / 2","100vh - _@_phi-vh","( 100vh - _@_phi-vh ) / 2",
				"61.803vw","_@_phi-vw / 2","100vw - _@_phi-vw","( 100vw - _@_phi-vw ) / 2"
			],
			"atoms":{
				"phi-width":{
					"group":"sizing",
					"example":"<div ... style='height: 100px; background-color:#fff;'></div>",
					"selector":".a-width-@var_name",
					"declaration_value":"width: _@_@var_name;"
				},
				"phi-height":{
					"group":"sizing",
					"example":"<div ... style='width: 100px; background-color:#fff;'></div>",
					"selector":".a-height-@var_name",
